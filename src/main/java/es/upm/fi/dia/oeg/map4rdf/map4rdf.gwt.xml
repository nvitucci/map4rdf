<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<module>
	<inherits name="com.google.gwt.user.User" />
    <inherits name="com.google.gwt.i18n.I18N"/>
	<inherits name="com.google.gwt.i18n.CldrLocales"/>
    <inherits name='name.alexdeleon.lib.gwtblocks.gwt-blocks' />
	<inherits name="org.gwtopenmaps.openlayers.OpenLayers"/>
	<inherits name='com.google.gwt.maps.Maps' />
	<inherits name="com.google.gwt.http.HTTP"/>
	<inherits name="com.google.gwt.user.theme.chrome.Chrome"/>
	<inherits name='com.googlecode.gwt.charts.Charts'/>
		
	<stylesheet src="/map4rdf.css"/>
	<stylesheet src="gwt_style.css"/>
	
	<source path="client" />
	<source path="share" />
	<public path="public" />
	
	<entry-point class="es.upm.fi.dia.oeg.map4rdf.client.Browser"></entry-point>
	
	<extend-configuration-property name="document.compatMode" value="BackCompat"/>
	<extend-property name="locale" values="es"/>
	<extend-property name="locale" values="en"/>
	<set-property-fallback name="locale" value="en"/>
 	<!--
    <set-property name="user.agent" value="gecko1_8" />
	-->
</module>